{
  "name": "vite-react-18-ssr-boilerplate",
  "version": "0.0.0",
  "private": true,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "ts-node src/server/server-dev.ts",
    "build": "ts-node scripts/build.ts",
    "start": "node dist/server/server-prod.js",
    "test": "vitest",
    "prettier": "prettier --check",
    "lint": "eslint --max-warnings 0",
    "typecheck": "tsc",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.?(c|m)@(j|t)s?(x)": [
      "yarn prettier --write",
      "yarn lint --fix"
    ],
    "*.{json,md,css,scss,yml}": "yarn prettier --write",
    "package.json": "sort-package-json"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@fastify/static": "^6.4.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/uuid": "^8.3.4",
    "@vanilla-extract/css-utils": "^0.1.2",
    "@vanilla-extract/dynamic": "^2.0.2",
    "cross-fetch": "^3.1.5",
    "fastify": "^4.2.0",
    "isbot": "^3.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-query": "^4.0.0-beta.23",
    "react-router-dom": "^6.4.0-pre.8",
    "type-fest": "^2.16.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@fastify/express": "^2.0.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/detect-port": "^1.3.2",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vanilla-extract/css": "^1.7.2",
    "@vanilla-extract/vite-plugin": "^3.2.1",
    "@vitejs/plugin-react": "^1.3.2",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "detect-port": "^1.3.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "fetch-mock": "^9.11.0",
    "husky": "^7.0.4",
    "jsdom": "^20.0.0",
    "lint-staged": "^12.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sort-package-json": "^1.57.0",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vitest": "^0.17.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
